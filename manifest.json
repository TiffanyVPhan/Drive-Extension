{
    "name": "Drive Extension",
    "version": "1.0",
    "description": "Change from personal email to Colorado email account on google apps",
    "author": "Tiffany Phan",
    "content_scripts": [
      {
        "matches":
        [
          "https://drive.google.com/*",
          "https://keep.google.com/",
          "https://calendar.google.com/calendar/r",
          "https://google.com",
          "https://www.photos.google.com"
        ],
        "js":
        [
          "redirect.js"
        ]
      }
    ],
    "permissions":
    [
        "activeTab",
        "tabs",
    ],
    "manifest_version": 2
}
